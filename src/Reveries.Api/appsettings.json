{
  "ExternalApis": {
    "Isbndb": {
      "ApiUrl": "https://api2.isbndb.com/",
      "ApiKey": "<YOUR_API_KEY>"
    },
    "GoogleBooks": {
      "ApiUrl": "https://www.googleapis.com/books/v1/",
      "ApiKey": "<YOUR_API_KEY>"
    }
  },
  "Postgres": {
    "Host": "192.168.0.35",
    "Port": 5432,
    "Database": "reveries_db",
    "Username": "reveries_user",
    "Password": "<YOUR_PASSWORD>",
    "ConnectionString": "",
    "Pool": {
      "Min": 1,
      "Max": 100
    },
    "TimeoutSeconds": 15,
    "CommandTimeoutSeconds": 20
  },
  "Redis": {
    "Host": "192.168.0.35",
    "Port": 6379,
    "Password": "<YOUR_PASSWORD>",
    "ConnectionString": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Enrichers.Environment", "Serilog.Formatting.Compact" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithEnvironmentName", "WithProcessId", "WithThreadId" ],
    "Properties": { "app": "reveries-api" }
  }
}
